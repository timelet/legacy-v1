{
  "files": [
    "./src/serviceWorkerSetup.ts"
  ],
  "compilerOptions": {
    "outDir": "./build",
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "ESNext", "WebWorker"],
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "remove"
  }
}
